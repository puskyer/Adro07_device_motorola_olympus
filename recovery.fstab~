# mount point	fstype		device			[device2]

/boot			emmc		/dev/block/mmcblk0p11
/cache			ext4		/dev/block/mmcblk0p15
/data			ext4		/dev/block/mmcblk0p16
/sdcard			vfat		/dev/block/mmcblk1p1   /dev/block/mmcblk1   flags=display="Micro SDcard";storage;wipeingui;removable
/emmc       vfat    	/dev/block/mmcblk0p18   flags=display="Internal SDcard";storage;wipeingui
/system			ext4		/dev/block/mmcblk0p12
/recovery		emmc		/dev/block/mmcblk0p10
/osh        	ext3		/dev/block/mmcblk0p13   flags=display="Webtop";backup=1;canbewiped
/preinstall 	ext3		/dev/block/mmcblk0p17

